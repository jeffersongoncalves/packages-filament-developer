{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.3",
        "jeffersongoncalves/filament-ace-editor-field": "1.x-dev",
        "jeffersongoncalves/filament-cep-field": "2.x-dev",
        "jeffersongoncalves/filament-check-whois-widget": "2.x-dev",
        "jeffersongoncalves/filament-cookie-consent": "2.x-dev",
        "jeffersongoncalves/filament-fathom": "2.x-dev",
        "jeffersongoncalves/filament-gtag": "2.x-dev",
        "jeffersongoncalves/filament-gtm": "2.x-dev",
        "jeffersongoncalves/filament-hidden-action": "1.x-dev",
        "jeffersongoncalves/filament-keyable": "2.x-dev",
        "jeffersongoncalves/filament-logo": "2.x-dev",
        "jeffersongoncalves/filament-matomo": "2.x-dev",
        "jeffersongoncalves/filament-multifactor-whatsapp": "1.x-dev",
        "jeffersongoncalves/filament-one-time-operations": "2.x-dev",
        "jeffersongoncalves/filament-pixel": "2.x-dev",
        "jeffersongoncalves/filament-plausible": "2.x-dev",
        "jeffersongoncalves/filament-qrcode-field": "2.x-dev",
        "jeffersongoncalves/filament-refresh-sidebar": "1.x-dev",
        "jeffersongoncalves/filament-topbar": "2.x-dev",
        "jeffersongoncalves/filament-umami": "2.x-dev",
        "jeffersongoncalves/filament-whatsapp-widget": "2.x-dev",
        "jeffersongoncalves/laravel-cep": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-cookie-consent": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-created-by": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-fake-cartoons": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-fathom": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-gtag": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-gtm": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-matomo": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-pixel": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-plausible": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-umami": "dev-master as 1.0.0",
        "jeffersongoncalves/laravel-whatsapp-widget": "dev-master as 1.0.0",
        "joaopaulolndev/filament-edit-profile": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.7",
        "wallacemartinss/filament-icon-picker": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.8",
        "laravel/boost": "^1.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.2",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "pnpm install",
            "pnpm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan icons:cache"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=livewire:assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan boost:update --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate -q",
            "@php artisan ide-helper:meta -q",
            "@php artisan ide-helper:model --write --reset -q -n",
            "./vendor/bin/pint --config=pint.json -q"
        ],
        "pint": [
            "./vendor/bin/pint --config=pint.json -q"
        ],
        "phpstan": [
            "./vendor/bin/phpstan"
        ],
        "schedule": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd\" \"php artisan schedule:work\" --names=schedule"
        ],
        "queue": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd\" \"php artisan queue:listen --tries=1\" --names=queue"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
